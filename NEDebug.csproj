	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\nedebug.pack">
			<CustomToolNamespace>NEP.NEDebug</CustomToolNamespace>
		</EmbeddedResource>
	</ItemGroup>
	
	<PropertyGroup>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<EnableDefaultCompileItems>false</EnableDefaultCompileItems>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="src\Benchmark\NEBenchmark.cs" />
		<Compile Include="src\Benchmark\NEProfiledMethod.cs" />
		<Compile Include="src\Console\NECommandTokenizer.cs" />
		<Compile Include="src\Console\NECommandToken.cs" />
		<Compile Include="src\Console\NEConsole.cs" />
		<Compile Include="src\Console\NEConsoleCommand.cs" />
		<Compile Include="src\Console\NEGenericCommands.cs" />
		<Compile Include="src\Core.cs" />
		<Compile Include="src\Drawing\NEDebugDrawer.cs" />
		<Compile Include="src\Drawing\NEDraw.cs" />
		<Compile Include="src\Drawing\NEDrawCommand.cs" />
		<Compile Include="src\Logging\NELog.cs" />
	</ItemGroup>
	
	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="COPY &quot;$(TargetPath)&quot; &quot;$(BONELAB_DIR)\Mods&quot;" />
	</Target>
</Project>